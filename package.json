{
  "name": "qckfx",
  "version": "0.1.4",
  "description": "A cli-based AI software engineering agent",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "qckfx": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "test:ui": "vitest run --config vitest.config.ts",
    "test:ui:watch": "vitest --config vitest.config.ts",
    "test:ui:coverage": "vitest run --config vitest.config.ts --coverage",
    "lint": "eslint 'src/**/*.ts' --ignore-pattern 'src/ui/**'",
    "lint:ui": "eslint 'src/ui/**/*.{ts,tsx}'",
    "lint:all": "eslint src/**/*.{ts,tsx}",
    "build": "tsc --project tsconfig.core.json",
    "build:ui": "vite build",
    "dev:ui": "vite",
    "dev": "concurrently \"npm run build:watch\" \"npm run dev:ui\"",
    "build:watch": "tsc --project tsconfig.core.json --watch",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build:complete": "npm run build && npm run build:ui",
    "typecheck": "tsc --project tsconfig.core.json --noEmit",
    "typecheck:ui": "tsc -p tsconfig.ui.json --noEmit",
    "typecheck:all": "npm run typecheck && npm run typecheck:ui",
    "ui:generate": "npx shadcn-ui@latest add",
    "start:dist": "node dist/cli.js",
    "prepublishOnly": "npm run build:complete"
  },
  "keywords": [
    "ai",
    "agent",
    "framework",
    "tools",
    "composition",
    "claude",
    "anthropic"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@e2b/code-interpreter": "^1.0.4",
    "@types/body-parser": "^1.19.5",
    "@types/connect-history-api-fallback": "^1.5.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/prompts": "^2.4.9",
    "@types/uuid": "^10.0.0",
    "body-parser": "^1.20.3",
    "commander": "^13.1.0",
    "connect-history-api-fallback": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "events": "^3.3.0",
    "express": "^4.21.2",
    "glob": "^8.1.0",
    "ora": "^8.2.0",
    "prompts": "^2.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "@types/react": "^18.2.71",
    "@types/react-dom": "^18.2.22",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.1",
    "autoprefixer": "^10.4.19",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "jsdom": "^24.0.0",
    "lucide-react": "^0.359.0",
    "msw": "^2.2.3",
    "postcss": "^8.4.38",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.0",
    "tailwind-merge": "^2.2.2",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "vite": "^5.2.6",
    "vite-plugin-html": "^3.2.2",
    "vitest": "^1.4.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
